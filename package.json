{
  "name": "jamescostian.com",
  "description": "Source code for jamescostian.com",
  "version": "1.0.0",
  "author": "James Costian <james@jamescostian.com>",
  "bugs": {
    "url": "https://github.com/jamescostian/jamescostian.com/issues"
  },
  "dependencies": {
    "cat-game": "jamescostian/cat-game#v1.0.0",
    "cssnano": "3",
    "glob": "7",
    "postcss-cli": "4",
    "postcss-custom-media": "6",
    "postcss-css-variables": "0.7",
    "postcss-import": "10",
    "showdown": "1"
  },
  "homepage": "https://jamescostian.com/",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamescostian/jamescostian.com.git"
  },
  "scripts": {
    "build-css": "postcss --map --output bundle.css css/home/main.css && postcss --map --output blog/bundle.css css/blog/main.css",
    "build-html": "node blog/htmlify.js",
    "build": "npm run build-css && npm run build-html"
  }
}
