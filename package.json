{
	"name": "jamescostian.com",
	"private": true,
	"description": "Source code for jamescostian.com",
	"version": "2.0.0",
	"author": "James Costian <james@jamescostian.com>",
	"scripts": {
		"start": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"gh-pages": "yarn build && git worktree add gh-pages ../jamescostian.com-gh-pages && mv ../jamescostian.com-gh-pages/.git public && mv public ../jamescostian.com-gh-pages && cd ../jamescostian.com-gh-pages && git commit -m \"Build $(git rev-parse HEAD)\" && git push && git worktree remove .",
		"type-check": "tsc --noEmit",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"prepare": "husky install"
	},
	"bugs": {
		"url": "https://github.com/jamescostian/jamescostian.com/issues"
	},
	"dependencies": {
		"@emotion/react": "^11.1.5",
		"@iconify-icons/jam": "^1.1.0",
		"@iconify/icons-ic": "^1.1.3",
		"@iconify/react": "^1.1.4",
		"@types/node": "^15.0.0",
		"@types/react": "^17.0.4",
		"@types/react-dom": "^17.0.3",
		"@types/react-helmet": "^6.1.1",
		"gatsby": "^3.3.1",
		"gatsby-plugin-emotion": "^6.3.0",
		"gatsby-plugin-feed": "^3.2.0",
		"gatsby-plugin-google-fonts": "^1.0.1",
		"gatsby-plugin-image": "^1.2.1",
		"gatsby-plugin-manifest": "^3.2.0",
		"gatsby-plugin-offline": "^4.2.0",
		"gatsby-plugin-react-helmet": "^4.2.0",
		"gatsby-plugin-sharp": "^3.3.1",
		"gatsby-plugin-ts": "^2.7.1",
		"gatsby-remark-copy-linked-files": "^3.2.0",
		"gatsby-remark-images": "^4.2.0",
		"gatsby-remark-prismjs": "^4.2.0",
		"gatsby-remark-responsive-iframe": "^3.2.0",
		"gatsby-remark-smartypants": "^3.2.0",
		"gatsby-source-filesystem": "^3.2.0",
		"gatsby-transformer-remark": "^3.2.0",
		"gatsby-transformer-sharp": "^3.3.0",
		"prismjs": "^1.23.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.1.0",
		"typescript": "^4.2.4"
	},
	"devDependencies": {
		"husky": "6",
		"lint-staged": "10",
		"prettier": "2.2.1"
	},
	"homepage": "https://jamescostian.com/",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jamescostian/jamescostian.com.git"
	}
}
